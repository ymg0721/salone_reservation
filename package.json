{
  "name": "nuxt-scaffold",
  "version": "1.0.0",
  "private": true,
  "engines": {
    "node": "20.x"
  },
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "prebuild": "npx only-allow pnpm",
    "build": "run-s build:check build:api \"build:app --dotenv .env.{1}\" --",
    "build:check": "node ./scripts/build-check.mjs",
    "build:api": "aspida",
    "build:app": "nuxi build",
    "predev": "npx only-allow pnpm",
    "dev": "run-p dev:*",
    "dev:api": "aspida --watch",
    "dev:app": "nuxi dev",
    "lint": "eslint .",
    "typecheck": "nuxi typecheck",
    "generate": "run-s build:api \"generate:app --dotenv .env.{1}\" --",
    "generate:api": "aspida",
    "generate:app": "nuxi generate",
    "preview": "nuxi preview",
    "postinstall": "nuxi prepare",
    "analyze": "nuxi analyze",
    "prepare": "husky install"
  },
  "dependencies": {
    "@aspida/axios": "^1.13.3",
    "@googlemaps/js-api-loader": "^1.16.6",
    "@nuxtjs/robots": "^3.0.0",
    "@pinia/nuxt": "^0.5.1",
    "@sentry/vue": "^7.34.0",
    "@turf/turf": "^6.5.0",
    "@vueuse/core": "^10.9.0",
    "@vueuse/nuxt": "^10.9.0",
    "@vueuse/router": "^10.9.0",
    "aspida": "^1.13.3",
    "axios": "^1.4.0",
    "body-scroll-lock": "4.0.0-beta.0",
    "cookie": "^0.5.0",
    "dayjs": "^1.11.9",
    "echarts": "^5.5.0",
    "gsap": "^3.12.5",
    "js-cookie": "^3.0.5",
    "nuxt-gtag": "^2.0.6",
    "nuxt-jsonld": "^2.0.8",
    "nuxt-swiper": "^1.2.2",
    "pinia": "^2.1.7",
    "qs": "^6.11.2",
    "swiper": "^11.0.7",
    "vue": "^3.4.21",
    "vue-echarts": "^6.6.9",
    "vue-router": "^4.3.0",
    "vue3-google-map": "^0.20.0"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.8.1",
    "@css-render/vue3-ssr": "^0.15.12",
    "@iconify-json/mdi": "^1.1.53",
    "@nuxt/devtools": "^1.0.8",
    "@nuxt/image": "^1.3.0",
    "@nuxtjs/eslint-config-typescript": "^12.1.0",
    "@nuxtjs/i18n": "^8.1.1",
    "@nuxtjs/sitemap": "^5.1.5",
    "@pinia-plugin-persistedstate/nuxt": "^1.2.0",
    "@sentry/tracing": "^7.34.0",
    "@types/body-scroll-lock": "^3.1.2",
    "@types/cookie": "^0.5.1",
    "@types/http-proxy-middleware": "^1.0.0",
    "@types/js-cookie": "^3.0.3",
    "@types/node": "^20.4.8",
    "@types/qs": "^6.9.7",
    "@zadigetvoltaire/nuxt-gtm": "^0.0.13",
    "eslint": "^8.54.0",
    "eslint-config-prettier": "^8.10.0",
    "husky": "^8.0.3",
    "lint-staged": "^13.2.3",
    "npm-run-all": "^4.1.5",
    "nuxt": "3.10.3",
    "postcss": "^8.4.27",
    "postcss-html": "^1.5.0",
    "prettier": "^3.0.1",
    "sass": "^1.64.2",
    "stylelint": "^15.10.2",
    "stylelint-config-standard-scss": "^10.0.0",
    "typescript": "^5.3.3",
    "unocss": "^0.58.5",
    "unplugin-vue-components": "^0.26.0",
    "vite": "^5.1.5",
    "vue-tsc": "^2.0.4"
  },
  "lint-staged": {
    "*.json": "prettier --write",
    "*.{css,scss}": [
      "prettier --write",
      "stylelint --fix"
    ]
  },
  "eslintConfig": {
    "extends": "@antfu"
  }
}
